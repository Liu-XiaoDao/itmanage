<% provide :c_active, "home" %>





<div class="container" style="padding-top:70px;" >
    <div class="row">
    	<div class="col-md-3">
	        <div class="panel panel-primary">
	        	<div class="panel-heading"><h3 class="panel-title">设备数量</h3></div>
	          	<ul class="list-group">
					<% @devicesstatus.each do |dstatus|%>
						<li class="list-group-item"><%= device_status dstatus.status %>设备:&nbsp;<span class="badge"><%= dstatus.count %></span></li>
					<% end %>
				</ul>
	      	</div>
    	</div>

    </div>
</div>
